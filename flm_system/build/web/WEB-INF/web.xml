    </servlet>
    <servlet>
        <servlet-name>PermissionSetting</servlet-name>
        <servlet-class>system_admin.PermissionSetting</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PassResetController</servlet-name>
        <servlet-class>guest.PassResetController</servlet-class>
