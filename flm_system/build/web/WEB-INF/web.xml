    <servlet>
        <servlet-name>LoadAvatarController</servlet-name>
        <servlet-class>guest.LoadAvatarController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCurriculumController</servlet-name>
        <servlet-class>guest.ViewCurriculumController</servlet-class>
