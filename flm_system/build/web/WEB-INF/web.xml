        <servlet-name>PloDetail</servlet-name>
        <url-pattern>/plo-detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MappingPloPo</servlet-name>
        <url-pattern>/po-plo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Plo_Excel</servlet-name>
        <url-pattern>/plo-excel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MappingSubject</servlet-name>
        <url-pattern>/subject-plo</url-pattern>
    </servlet-mapping>
